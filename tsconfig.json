    "target": "es2016",
    "jsx": "react-jsx",
    "module": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "outDir": "./dist",
    "resolveJsonModule": true
